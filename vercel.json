{
    "version": 2,
    "builds": [
      {
        "src": "project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python3.12",
          "installCommand": "pip install psycopg2-binary==2.9.9 --no-cache-dir && pip install -r requirements.txt --no-cache-dir",
          "maxLambdaSize": "15mb"
        }
      },
      {
        "src": "staticfiles/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "project/wsgi.py"
      }
    ]
  }