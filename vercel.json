{
    "version": 2,
    "builds": [
      {
        "src": "your_project/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "includeFiles": ["staticfiles/**", "static/**"]
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }